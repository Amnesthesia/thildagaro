<h2><%= @project.title %></h2>

<%= form_for @project, :url => casein_project_path(@project), html: {multipart: true} do |f| %>

	<%= render :partial => 'casein/projects/form', :locals => { :f => f } %>

	<p class="submits">
	  	<%= link_to 'Avbryt', casein_projects_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Spara", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Tillbaka".html_safe, casein_projects_path %></li>
<% end %>
